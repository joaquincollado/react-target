{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "*": ["src/*"],
      "assets/*": ["src/assets/*"],
      "components/*": ["src/components/*"],
      "locales/*": ["src/locales/*"],
      "styles/*": ["src/styles/*"],
      "store/*": ["src/redux/*"],
      "actions/*": ["src/redux/actions/*"],
      "reducers/*": ["src/redux/reducers/*"],
      "api/*": ["src/api/*"]
    }
  },
  "exclude": ["node_modules"]
}
